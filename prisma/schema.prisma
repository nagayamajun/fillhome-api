// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Owner {
  id    String     @id @default(cuid())
  firebase_uid  String @unique
  email String     @unique 
  first_name  String
  last_name String
  phone_number String
}


